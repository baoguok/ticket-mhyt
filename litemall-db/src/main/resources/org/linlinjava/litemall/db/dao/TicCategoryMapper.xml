    <!--  窗口查询余票信息  -->
    <select id="WinGoodList" resultMap="CateAndGoodRstMap">
    SELECT * ,goo.goo_total-IFNULL((
      select sum(b.org_number)
      from tic_order_goods b
      left join tic_order c
      on c.ord_id = b.org_order_id
      where b.org_goods_id = goo.goo_id
      and (c.ord_order_status = 201 or c.ord_order_status = 301 or c.ord_order_status = 101)
      and to_days(c.ord_confirm_time) = to_days(#{Time ,jdbcType = VARCHAR})),0) stock FROM tic_category cate
      left JOIN tic_goods goo ON cate.cat_id = goo.goo_cat_id
      WHERE cat_is_on_sale = 0
    AND cat_deleted = 0 order by cat_sort_order
 </select>
</mapper>